@import '~@/assets/iconfont/iconfont.css';
@import './element-variables.scss';
//头部菜单激活背景颜色
$menuActiveBgColor: #4150cb;
html,
body {
	height: 100%;
	overflow: hidden;
	margin: 0;
	padding: 0;
}

#app > .el-container {
	height: 100%;
}

.wx-container {
	padding: 40px 60px;
	text-align: center;
	.scan {
		color: #999;
	}
	h3 {
		color: $--color-primary;
		font-size: 28px;
		font-weight: normal;
	}
}
//朗慧图标，文字与图标间距
.el-icon-lh-span span{
  margin-left:5px;
}
